for (let i = 1; i <= 100; i++) {
    if(i%3==0){
        console.log("El valor del numero es: %d  Fizz",i);
    }
    else if(i%5==0){
        console.log("El valor del numero es :%d  Buzz",i);
      } 
      
    if(i%3==0 && i%5==0){
        console.log("El valor del numero es: %d  FizzBuzz",i);
    }else{
        console.log("El valor del numero es :%d",i);
    }
}